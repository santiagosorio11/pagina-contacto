{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(assets|fonts)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public,max-age=604800,immutable"
        }
      ]
    },
    {
      "source": "/(css|js)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public,max-age=86400"
        }
      ]
    }
  ]
}
